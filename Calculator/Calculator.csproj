<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<AssemblyName>Calculator</AssemblyName>
		<RootNamespace>Calculator</RootNamespace>
		<ApplicationIcon>Images\calculator.ico</ApplicationIcon>
		<Authors>Daniil Shipilin</Authors>
		<Description>A simple basic calculator.</Description>
		<Copyright>Daniil Shipilin</Copyright>
		<Company>Illuminati Software Inc.</Company>
		<Version>1.0.0</Version>
		<Product>Calculator</Product>
		<RepositoryUrl>https://github.com/daniilshipilin/Calculator</RepositoryUrl>
		<PackageProjectUrl>https://github.com/daniilshipilin/Calculator</PackageProjectUrl>
		<PackageIcon>calculator.png</PackageIcon>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<None Include="Images\calculator.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="ApplicationUpdater" Version="0.3.0" />
		<PackageReference Include="GitVersion.MsBuild">
			<Version>5.10.3</Version>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
			<PrivateAssets>all</PrivateAssets>
		</PackageReference>
		<PackageReference Include="MathExpressionParser" Version="0.2.0" />
		<PackageReference Include="Newtonsoft.Json">
			<Version>13.0.1</Version>
		</PackageReference>
	</ItemGroup>

	<PropertyGroup />
</Project>
